{
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json",
        "name": "lskc-custom/order-query",
        "title": "LSKC Order Query",
        "category": "widgets",
        "icon": "product",
        "description": "Query WooCommerce orders.",
	"supports": {
		"interactivity": {
			"clientNavigation": true
		},
		"align": true,
		"reusable": false,
		"inserter": false
	},
        "providesContext": {
                "lskccustom/orderQuery": "query"
        },
        "attributes": {
                "query": {
                        "type": "object",
                        "default": {
                                "perPage": 10,
                                "order": "desc",
                                "orderBy": "date",
                                "statuses": ["completed", "processing"]
                        }
                }
        },
	"keywords": [ "WooCommerce" ],
	"usesContext": [ "postId", "postType", "queryId" ],
	"textdomain": "lskc-custom"
}
